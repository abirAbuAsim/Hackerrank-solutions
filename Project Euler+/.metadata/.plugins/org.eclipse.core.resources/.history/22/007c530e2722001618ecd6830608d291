import java.io.*;
import java.util.*;

public class Solution {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int T = input.nextInt();
		int N, sum, I, J, K;
		for (I = 1; I <= T; I++) {
			N = input.nextInt();
			sum = 0;
			for (J = 3; J < N; J += 3) {
				if(J % 3 == 0) {
					if (J % 5 == 0) {
						continue;
					}
					sum += J;
				}
			}
			for (J = 5; J < N; J += 3) {
				if(J % 5 == 0) {
					if (J % 3 == 0) {
						continue;
					}
					sum += J;
				}
			}
			System.out.println(sum);
		}
	}
}
