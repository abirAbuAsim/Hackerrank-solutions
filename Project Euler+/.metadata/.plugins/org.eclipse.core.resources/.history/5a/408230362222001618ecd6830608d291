import java.io.*;
import java.util.*;

public class Solution {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int T = input.nextInt();
		int N, sum;
		for (int I = 1; I <= T; I++) {
			N = input.nextInt();
			sum = 0;
			for (int J = 1; J <= N; J++) {
				if (J % 3 == 0 || J % 5 == 0) {
					sum++;
				}
			}
			System.out.println(sum);
		}
	}
}
