import java.io.*;
import java.util.*;

public class Solution {
	private int SumDivisibleBy(int n, int p) {
		int N = n / p;
		return n * N  * ((N + 1) / 2);
	}
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int T = input.nextInt();
		int N;
		int I, J;
		Solution ob;
		HashMap<Integer, Integer> numMap = new HashMap<Integer, Integer>();
		int sum;
		for (I = 1; I <= T; I++) {
			N = input.nextInt();
			ob = new Solution();
			sum = ob.SumDivisibleBy(3, N) + ob.SumDivisibleBy(5, N) - ob.SumDivisibleBy(15, N);
			System.out.println(sum);
		}
	}
}
