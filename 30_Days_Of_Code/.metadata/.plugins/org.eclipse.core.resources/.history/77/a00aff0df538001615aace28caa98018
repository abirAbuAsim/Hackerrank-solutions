 */
import java.io.*;
import java.util.*;
public class Solution {
	public static void main(String[] args) {
		System.out.println("5");
		System.out.println("4 3");
		System.out.println("-2 0 1 2 3"); // YES
		System.out.println("5 2");
		System.out.println("5 2 3 -4 0"); // NO
		System.out.println("6 4");
		System.out.println("5 6 2 -10 0 4"); // YES
		System.out.println("3 1");
		System.out.println("-200 0 150"); // NO
		System.out.println("10 6");
		System.out.println("-1 2 0 4 -2 6 7 -8 9 0"); // YES
	}
}
